k_order":["row_QXgrNf","row_tJ64Nz","row_B8FVLN"],"disabled":true,"settings":{"title":"Results","heading_size":"h1","text":"<p>Talk about results of your customers and how your product improved their life.<\/p>","layout":"results_second","results_headline":"Results heading","results_headline_size":"h2","results_headline_alignment":"left","caption":"<p>Caption about the results and\/or link for their proof.<\/p>","border_color":"#0d48e9","padding_top":72,"padding_bottom":36}},"image_banner_Vjmm3T":{"type":"image-banner","name":"t:sections.image-banner.presets.name","settings":{"image":"shopify:\/\/shop_images\/ChatGPT_Image_12_dic_2025_07_47_56_p.m..png","image_overlay_opacity":0,"image_height":"adapt","desktop_content_position":"middle-center","show_text_box":true,"desktop_content_alignment":"center","color_scheme":"","image_behavior":"none","mobile_content_alignment":"center","stack_images_on_mobile":true,"show_text_below":true}},"rich_text_JMWVdV":{"type":"rich-text","blocks":{"text_AjW9Yt":{"type":"text","settings":{"text":"<h3>CINEMA-GRADE IMAGE<\/h3><p>• Native Full HD 1080p resolution (supports 4K input)<br\/>• Approx. 2000 lumens for clear images in low light<br\/>• Projection size from 30\" to 130\"<br\/>• Perfect for wall or ceiling movie nights<\/p><h3>ANDROID SMART OS<\/h3><p>• Built-in Android operating system<br\/>• Install apps like Netflix, YouTube, Prime Video and more<br\/>• Stream directly without extra devices<br\/>• Wireless screen mirroring for Android and iOS phones<\/p><h3>FLEXIBLE PROJECTION<\/h3><p>• Projection distance: 0.4–3 meters<br\/>• Automatic keystone correction for straight images<br\/>• 1.4× manual zoom to fine-tune screen size<br\/>• Supports ceiling, rear, mirror and standard mounting<\/p><h3>CONNECTIVITY & STREAMING<\/h3><p>• HDMI, USB and 3.5 mm audio ports<br\/>• Works with laptops, game consoles and USB drives<br\/>• WiFi and Bluetooth for wireless connections<br\/>• Easy to pair with external speakers or soundbars<\/p><h3>AUDIO & GAMING<\/h3><p>• Built-in stereo speakers<br\/>• Great for movies, series, sports and casual gaming<br\/>• Compatible with game sticks and consoles for home gaming setups<\/p><h3>POWER & USAGE<\/h3><p>• No built-in battery (requires AC\/DC power)<br\/>• Best performance in dark or low-light rooms<br\/>• Ideal for bedrooms, living rooms and media corners<\/p><h3>WHAT’S IN THE BOX<\/h3><p>• 1 × LuxMax3 Smart Projector<br\/>• 1 × Remote control<br\/>• 1 × HDMI cable<br\/>• 1 × Power adapter<br\/>• 1 × User manual<\/p>"}}},"block_order":["text_AjW9Yt"],"name":"t:sections.rich-text.presets.name","settings":{"desktop_content_position":"left","content_alignment":"left","color_scheme":"","full_width":true,"padding_top":0,"padding_bottom":0}},"testimonials_RPWf8i":{"type":"testimonials","blocks":{"column_DETLhB":{"type":"column","settings":{"title":"This projector blew me away.","text":"<p>The image is super clear even in a semi-lit room, and the two controllers make it perfect for gaming or movie nights with friends. Setup took like five minutes. Honestly feels like having a mini theater at home.<\/p>","image_pos":"below_description","author_avatar":"shopify:\/\/shop_images\/person-stands-on-rocks-poking-out-of-the-ocean-shoreline.jpg","author":"<strong>Eric Dawson<\/strong>"}},"column_4xMFRq":{"type":"column","settings":{"title":"Really impressed with how bright and sharp the picture is.","text":"<p>The controllers respond well, and multiplayer games run great. Sound is decent, but I hooked it to my speaker and now it feels next-level. Totally worth it.<\/p>","image_pos":"below_description","author_avatar":"shopify:\/\/shop_images\/silhouetted-couples-look-out-at-a-city-at-night.jpg","author":"<strong>Michael Grant<\/strong>"}},"column_h6ArLQ":{"type":"column","settings":{"title":"I’ve tried a few projectors before, but this one is easily the best in this price range.","text":"<p>Smooth connection, good colors, and the controllers make it super fun for retro-style games. My kids and I use it almost every night.<\/p>","image_pos":"below_description","author_avatar":"shopify:\/\/shop_images\/self-serve-coffee-shop.jpg","author":"<strong>Andrew Blake<\/strong>"}},"column_3LqeV4":{"type":"column","settings":{"title":"Solid projector all around.","text":"<p>The quality is clean, the controllers work great, and it’s bright enough to use even without turning off every light. Perfect for movie nights, games, and parties. Zero regrets.<\/p>","image_pos":"below_description","author_avatar":"shopify:\/\/shop_images\/person-sits-on-their-couch-next-to-their-dog.jpg","author":"<strong>Hunter Brooks<\/strong>"}}},"block_order":["column_DETLhB","column_4xMFRq","column_h6ArLQ","column_3LqeV4"],"settings":{"title":"Reviews","testimonials_per_row":4,"gap":12,"show_stars":true,"stars_color":"#ffcc00","background_color":"#eeeef6","show_quotes":true,"padding_top":32,"padding_bottom":36}},"section_divider_7c9mQN":{"type":"section-divider","settings":{"shape":"waves_3","flip_horizontal":false,"flip_vertical":false,"background_color":"#ffffff","shape_color":"#212240","padding_top":0,"padding_bottom":0}}},"order":["colors_changer_aDXkpK","main","frequently_bought_together_z4GahM","builder_p98UdB","builder_JUkPnd","comparison_table_8w9TDA","horizontal_ticker_RyAwyh","results_WgRpTd","image_banner_Vjmm3T","rich_text_JMWVdV","testimonials_RPWf8i","section_divider_7c9mQN"]}